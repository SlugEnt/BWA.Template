@using SlugEnt.BWA.Client.Layout

<Router AppAssembly="typeof(Program).Assembly" 
        AdditionalAssemblies="new[] {typeof(_Imports).Assembly,
        typeof(TestA).Assembly}">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(InitialLayout)">
            <NotAuthorized>
                <RedirectToLogin />
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>
